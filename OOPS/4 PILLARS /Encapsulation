#include <iostream>

using namespace std;

class student{
    
    //implementing encapsuation by hiding data members  and making it private
    
    private :
    int age;
    string name;
    
    public:
    int getage(){
        return this->age;
    }
    void setage(int num){
        age = num;
    }
    
    void print(){
        cout<<age<<endl;
    }
    
    
};

int main()
{
    student s;
    s.setage(30);
    cout<<"age is "<<s.getage();

    return 0;
}
