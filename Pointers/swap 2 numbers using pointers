#include <stdio.h>

void swap(int* ,int*);

void swap(int *x ,int *y ){
    
    int temp;
    temp = *x ;
    *x = *y;
    *y =temp;
    
}

int main()
{
    int a,b;
    printf("enter 2 variables :");
    scanf("%d%d",&a,&b);
    
    swap(&a,&b);
    printf("swappwed a = %d , b = %d", a,b);
    

    return 0;
}

